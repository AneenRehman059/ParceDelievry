<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="style_edittext">
        <item name="android:textSize">14sp</item>
        <item name="android:textColorHint">@color/ed_hint_color</item>
        <item name="android:textColor">@color/dialog_text_color</item>
        <item name="android:background">@drawable/rectangel_shape</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
    </style>

    <style name="style_button_blue">
        <item name="android:background">@drawable/button_style</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="NavigationTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="itemIconTint">@android:color/white</item>
        <item name="itemTextColor">@android:color/white</item>
        <item name="android:background">#00ffffff</item>
    </style>




</resources>